function solve(arr) {
    let n = +arr.shift()
    let leftSum = 0;
    let rightSum = 0;
    for(let i =0; i< n; i++) {
       leftSum += Number(arr[i])
    }
    for(let i = n; i<arr.length; i++) {
        rightSum+=Number(arr[i])

    }
    return [leftSum,rightSum]
}


console.log(
    solve([2,10,90,60,40])
)

->[100,100]
